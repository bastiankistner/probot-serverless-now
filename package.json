{
  "name": "@sanalytics/probot-serverless-now",
  "version": "1.0.3",
  "description": "An extension for running Probot on now.sh v2",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "format": "prettier --config prettier.config.js --write 'src/**/*.{ts,js}'"
  },
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/passionkind/probot-serverless-now.git"
  },
  "author": "Bastian Kistner",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/passionkind/probot-serverless-now/issues"
  },
  "homepage": "https://github.com/passionkind/probot-serverless-now#readme",
  "engines": {
    "node": ">= 8.10.0",
    "npm": ">= 5.0.0"
  },
  "keywords": [
    "zeit",
    "now.sh",
    "probot",
    "github"
  ],
  "devDependencies": {
    "@types/jest": "^23.3.10",
    "@types/micro": "^7.3.3",
    "@types/node": "^10.12.17",
    "jest": "^23.6.0",
    "prettier": "^1.15.3",
    "probot": "^7.4.0",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "typescript": "^3.2.2"
  },
  "dependencies": {
    "micro": "^9.3.3"
  },
  "peerDependencies": {
    "probot": "^7.4.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "roots": [
      "<rootDir>/src"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(j|t)sx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testEnvironment": "node",
    "setupFiles": [
      "./src/tests/setup.js"
    ]
  }
}
